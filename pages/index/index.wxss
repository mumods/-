.swiper{
  width: 100%;
  /* height: auto; */
  height: 340rpx;
}
.img{
  width: 100%;
}
.navigate{
  display: flex;
  width: 750rpx;
  padding: 20rpx;
}
.nav{
  flex:1
}
.navimg{
  width: 100%
}
.head{
  width: 100%;
}
.head image{
  width: 100%;
}
.content{
  overflow: hidden;
  padding: 0 0 18rpx 0;
  border-left: 10rpx solid #fff;
}
.content navigator{
  float: left;
  width: 33.33%;
}
.content navigator:nth-child(n+2){
  height: 208rpx;
  border-left: 8rpx solid #fff
}
.content navigator:nth-child(2){
  border-bottom: 8rpx solid #fff;
}
.content navigator:nth-child(3){
  border-bottom: 8rpx solid #fff;
  border-right: 10rpx solid #fff;
}
.content navigator:nth-child(5){
  border-right: 10rpx solid #fff;
}

.content navigator image{
 width: 100%;
 height: 100%;
}