.cart{
  width: 100%;
}
.get{
  width: 40%;
  margin:10rpx auto;
  padding: 10rpx;
  text-align: center;
  font-size: 30rpx;
  color: #eb5540;
  border: 2rpx solid #eb5540;
  border-radius: 10rpx;
}
.header{
  display: flex;
  padding-bottom: 20rpx;
  border-bottom: 3rpx solid #ddd;
}
.addressleft{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.iconfont{
  background-color: #eb5540;
  color: #fff;
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.addressright{
  flex: 6;
}
.address{
  display: flex;
  width: 100%;
  /* padding: 10rpx; */
}
.address view:nth-child(2){
  margin-left: 20rpx;
}
.address view:nth-child(2) text{
  font-size: 22rpx;
  color: #888;
}
.add{
  /* padding: 10rpx; */
  padding-top: 10rpx;
  display: flex;
}
.add view:nth-child(1){
  width: 60rpx;
  height: 37rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: #03af00;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
}
.content{
  margin: 10rpx;
}
.asd{
  width: 100%;
  height: 800rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.head{
  font-size: 40rpx;
  padding: 10rpx;
  /* border-top: 4rpx solid rgba(235,85,64,.5); */
  border-bottom: 4rpx solid rgba(235,85,64,.5);
}
.info{
  display: flex;
  border-bottom: 3rpx solid #ccc;
  padding: 10rpx;
}
.check{
  flex: 2;
  display: flex;
  align-items: center;
}
.check checkbox-group{
  transform: scale(0.7);
}
.img{
  flex: 3;
  display: flex;
  align-items: center;
}
.right{
  flex: 7;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding-left: 30rpx;
  justify-content: space-around;
}
.title{
  display:-webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #666;
}
.img image{
  width: 100%;
}
.pricecount{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.price{
  color: #eb5540;
  text-align: left;
}
.count{
  width: 30%;
  height: 30rpx;
  display: flex;
  justify-content: space-between;
}
.count view:nth-child(1){
  width: 35rpx;
  height: 35rpx;
  color: #555;
  border: 3rpx solid #ccc;  
  display: flex;
  align-items: center;
  justify-content: center;
}
.count view:nth-child(3){
  width: 35rpx;
  height: 35rpx;
  border: 3rpx solid #ccc;
  color: #555;
  display: flex;
  align-items: center;
  justify-content: center;
}
.count view:nth-child(2){
  height: 35rpx;
  display: flex;
  color: #555;
  align-items: center;
  justify-content: center;
}
.footer{
  display: flex;
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 70rpx;
}
.allcheck{
  flex: 4;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}
.allprice{
  flex: 4;
  display: flex;
  align-items: center;
}
.allprice view:nth-child(1){
  color: #888;
  font-size: 25rpx;
}
.allprice view:nth-child(2){
  color: #eb5540;
  padding-left: 20rpx;
}
.Checkout{
  flex: 2;
  background-color: #eb5540;
  color: #fff;
  font-size: 37rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.allcheck checkbox{
  vertical-align: middle;
  transform: scale(0.7);
}