page{
  height: 100%;
}
.container{
  display: flex;
  flex-direction: column;
  height: 100%
}
.content{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
}
.active{
  color: #eb5540;
}
.left{
  height: 100%;
  color: #777;
  text-align: center;
  width: 28%;
}
.left view{
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.right{
  height: 100%;
  width: 72%;
  color: #777;
  /* display: flex; */
  
}
.right_head{
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  color:#000;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.right navigator{
  width: 33.33%;
  text-align: center;
}

.right_content{
  display: flex;
  width: 100%;
  flex-wrap: wrap;
}
.right image{
  width: 50%;
}